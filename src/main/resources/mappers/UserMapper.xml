     <if test="loginname!=null and loginname!=''">
     and s.loginname like "%${loginname}%"
     </if>
     <if test="identity!=null and identity!=''">
     and  s.identity=#{identity}
     </if>
     <if test="phone!=null and phone!=''">
     and s.phone=#{phone}
     </if>
   </where>
  </select>
